{
  "AppSettings": {
    "Token": "bcdbce44-1d88-4ad1-b0ad-9fd7da41a304",
    "BaseUrl": "http://dev-wooliesx-recruitment.azurewebsites.net/api/resource",
    "ShopperHistoryEndPoint": "shopperHistory",
    "ProductsEndPointEndPoint": "products",
    "TrolleyCalculatorEndPoint": "trolleyCalculator"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Log-%COMPUTERNAME%-.txt",
          "outputTemplate": "{Timestamp:dd/MM/yyyy HH:mm:ss} {Application} [{Level}] [{ThreadId}] {SourceContext} : {Message}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1000000
        }
      }
    ],
    "Enrich": [
      "WithThreadId",
      "FromLogContext"
    ],
    "Properties": {
      "Application": "WooliesX.Web"
    }
  }
}
